{
  "name": "chihiro-workspace",
  "description": "",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm -F @chihiro/app build",
    "preview": "pnpm -F @terminus/t-console-web preview",
    "start": "pnpm -F @chihiro/app start",
    "clean-pkgs": "pnpm -r exec rm -rf dist .turbo .swc *.log node_modules",
    "clean": "pnpm clean-pkgs && rm -rf node_modules",
    "typecheck": "turbo run typecheck --filter=./packages/**/*",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --fix",
    "bump": "tsx scripts/bump.ts",
    "test": "vitest",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@types/lodash-es": "4.17.6",
    "@types/node": "^18.7.18",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/semver": "7.3.13",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "@vitejs/plugin-react": "3.0.0",
    "@vitejs/plugin-react-swc": "3.3.0",
    "clean-package": "2.1.1",
    "commitlint": "^17.1.2",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react": "1.1.7",
    "eslint-detailed-reporter": "^0.9.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "fast-glob": "3.2.11",
    "husky": "8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "2.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup-plugin-visualizer": "5.8.1",
    "sass": "1.54.4",
    "semver": "7.3.8",
    "tslib": "2.4.0",
    "tsup": "6.2.1",
    "tsx": "3.10.1",
    "turbo": "1.4.2",
    "typescript": "4.9.3",
    "vite": "4.3.1",
    "vite-plugin-dts": "2.3.0",
    "vite-plugin-banner": "0.7.0",
    "vitest": "0.29.1"
  },
  "pnpm": {
    "overrides": {
      "@tanstack/react-query": "4.2.3",
      "lodash-es": "4.17.21",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "react-router-dom": "6.10.0",
      "typescript": "4.9.3",
      "zustand": "4.3.6"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
